CPAN_AUTHOR="ingy"
inherit perl

NAME="perl-IO-All"
VERSION=0.86
RELEASE=1
CATEGORY="Perl"
SUMMARY="Perl IO module collection"
DESCRIPTION="IO::All combines all of the best Perl IO modules into a single
nifty object oriented interface to greatly simplify your everyday Perl IO
idioms. It exports a single function called io, which returns a new IO::All
object."

ARCH=noarch
